{
  "name": "uetuluk-portfolio",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "sync-assets": "for f in public/assets/*.png; do wrangler r2 object put portfolio-assets/assets/$(basename $f) --file=$f; done && wrangler r2 object put portfolio-assets/favicon.ico --file=public/favicon.ico",
    "deploy": "npm run build && npm run sync-assets && wrangler deploy"
  },
  "dependencies": {
    "@unhead/react": "^2.0.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.468.0",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^16.4.0",
    "react-responsive-masonry": "^2.7.1",
    "react-router-dom": "^7.0.0",
    "react-spinners": "^0.17.0",
    "tailwind-merge": "^2.5.5",
    "unhead": "^2.0.19"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.0",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-responsive-masonry": "^2.7.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.49",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.48.1",
    "vite": "^6.0.3",
    "vitest": "^4.0.15",
    "wrangler": "^4.53.0"
  }
}
